{
  "name": "epidemic-web-client",
  "version": "0.0.1",
  "description": "app description",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "start": "webpack-dev-server --progress --port 8081",
    "start:server": "http-server -a 0.0.0.0 -p 8081 ./dist",
    "build": "webpack -p --config ./webpack.production.config.js",
    "clean:report": "rimraf ./report",
    "clean": "rimraf ./dist",
    "test": "npm run clean:report && jest",
    "test:watch": "npm run clean:report && jest --watch",
    "test:ci": "npm run clean:report && jest --coverage"
  },
  "jest": {
    "coverageDirectory": "report",
    "setupTestFrameworkScriptFile": "<rootDir>/__mocks__/setup.js",
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/__mocks__/empty.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/report/",
      "/__mocks__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "testRegex": "\\.test\\.js$"
  },
  "author": "MC Solmu",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-babel-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.44.2",
    "flow-typed": "^2.1.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "jasmine-reporters": "^2.2.1",
    "jest": "^19.0.2",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "redux-devtools-extension": "^2.13.0",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "svg-sprite-loader": "^0.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "classnames": "^2.2.5",
    "connected-react-router": "^4.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "fetch-intercept": "^2.2.3",
    "flexboxgrid-sass": "^8.0.5",
    "history": "^4.6.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.18.1",
    "node.normalize.scss": "^3.0.3",
    "openlayers": "^4.1.1",
    "promise-polyfill": "^6.0.2",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-cookie": "^2.0.6",
    "react-dom": "^15.5.4",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "slideout": "^1.0.1",
    "whatwg-fetch": "^2.0.3"
  }
}
